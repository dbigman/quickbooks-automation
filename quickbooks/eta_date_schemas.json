{\n  \"input_schema\": {\n    \"$schema\": \"https://json-schema.org/draft/2020-12/schema\",\n    \"title\": \"ReadETAInput\",\n    \"type\": \"object\",\n    \"additionalProperties\": false,\n    \"properties\": {\n      \"model\": { \n        \"const\": \"sale.order\", \n        \"description\": \"Odoo model\" \n      },\n      \"ids\": {\n        \"type\": \"array\",\n        \"items\": { \"type\": \"integer\", \"minimum\": 1 },\n        \"minItems\": 1,\n        \"description\": \"Sale order record IDs\"\n      },\n      \"fields\": {\n        \"type\": \"array\",\n        \"items\": { \"type\": \"string\" },\n        \"const\": [\"eta_date\"],\n        \"description\": \"Must be ['eta_date']\"\n      }\n    },\n    \"required\": [\"model\", \"ids\", \"fields\"]\n  },\n  \"output_schema\": {\n    \"$schema\": \"https://json-schema.org/draft/2020-12/schema\",\n    \"title\": \"ETAResult\",\n    \"type\": \"object\",\n    \"additionalProperties\": false,\n    \"properties\": {\n      \"orders\": {\n        \"type\": \"array\",\n        \"items\": {\n          \"type\": \"object\",\n          \"additionalProperties\": false,\n          \"properties\": {\n            \"order_id\": { \"type\": \"integer\", \"minimum\": 1 },\n            \"eta_date\": { \"type\": [\"string\", \"null\"], \"format\": \"date\" }\n          },\n          \"required\": [\"order_id\", \"eta_date\"]\n        }\n      }\n    },\n    \"required\": [\"orders\"]\n  }\n}
